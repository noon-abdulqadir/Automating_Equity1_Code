@incollection{moore_immersion_2020,
  title = {Immersion},
  booktitle = {The {{International Encyclopedia}} of {{Media Psychology}}},
  author = {Moore, Melissa M. and Green, Melanie C.},
  date = {2020},
  pages = {1--6},
  publisher = {{John Wiley \& Sons, Ltd}},
  doi = {10.1002/9781119011071.iemp0059},
  url = {http://onlinelibrary.wiley.com/doi/abs/10.1002/9781119011071.iemp0059},
  urldate = {2022-02-09},
  abstract = {Immersion refers to the absorption individuals feel when reading a story, watching a movie, or playing a video game. Individuals are likely to experience immersion when they are engaged in the narrative world without distraction from situational influences (e.g., loud noises, software trouble) or unrealistic components of the narrative (e.g., incorrect information, poorly written or animated characters). An individual's likelihood of immersion may be increased through factors including prior knowledge of the subject, perceived realism of the narrative, and emotional realism related to the characters. Individual traits also play a role, such as one's need for affect or differences in one's ability to be transported into a narrative, referred to as transportability. Additional elements such as the quality of the narrative and situational factors should also be considered. One type of immersion is transportation into a narrative world, which occurs when a reader or viewer becomes fully absorbed in a narrative emotionally and cognitively. Similar types of narrative response include flow, presence, and narrative engagement. Immersion can be measured through scales designed to capture immersive media responses, such as the transportation scale, the narrative engagement scale, and the game engagement questionnaire.},
  isbn = {978-1-119-01107-1},
  langid = {english},
  keywords = {_tablet,absorption,engagement,flow,narrative,transportation},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119011071.iemp0059},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Moore & Green (2020). Immersion.pdf;/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Data Directory/storage/JDTRYMYA/9781119011071.html}
