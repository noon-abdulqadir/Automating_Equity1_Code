@article{liu2015CoalitionFormationTechnology,
  ids = {liuCoalitionFormationTechnology2015a},
  title = {Coalition Formation during Technology Adoption},
  author = {Liu, Fei and Dedehayir, Ozgur and Katzy, Bernhard},
