@article{wright2020SoftPowerHard,
  title = {Soft {{Power}}, {{Hard News}}: {{How Journalists}} at {{State-Funded Transnational Media Legitimize Their Work}}},
  shorttitle = {Soft {{Power}}, {{Hard News}}},
  author = {Wright, Kate and Scott, Martin and Bunce, Mel},
  date = {2020-10-01},
