@article{langer_highly_2020,
  title = {Highly Automated Interviews: Applicant Reactions and the Organizational Context},
  shorttitle = {Highly Automated Interviews},
  author = {Langer, Markus and König, Cornelius J. and Sanchez, Diana Ruth-Pelipez and Samadi, Sören},
