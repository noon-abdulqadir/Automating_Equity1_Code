@article{Wacker1998,
  ids = {wackerDefinitionTheoryResearch1998,wackerDefinitionTheoryResearch1998a,wackerDefinitionTheoryResearch2002},
  title = {A Definition of Theory: Research Guidelines for Different Theory-Building Research Methods in Operations Management},
  author = {Wacker, John G},
  date = {2002},
  journaltitle = {Journal of Operations Management},
  volume = {16},
