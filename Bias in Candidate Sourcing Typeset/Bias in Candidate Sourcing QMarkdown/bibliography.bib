@article{elgar2005ValiditySelfreportedHeight,
  ids = {ValiditySelfreportedHeight2005},
  title = {Validity of Self-Reported Height and Weight and Predictors of Bias in Adolescents},
  author = {Elgar, Frank J. and Roberts, Chris and Tudor-Smith, Chris and Moore, Laurence},
  date = {2005},
  journaltitle = {Journal of Adolescent Health},
