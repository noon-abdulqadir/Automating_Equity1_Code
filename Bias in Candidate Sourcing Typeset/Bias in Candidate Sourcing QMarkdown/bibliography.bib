  editor = {Escalante, Hugo Jair and Escalera, Sergio and Guyon, Isabelle and Baró, Xavier and Güçlütürk, Yağmur and Güçlü, Umut and family=Gerven, given=Marcel, prefix=van, useprefix=true},
