@article{jonkman2020BufferingNegativeNews,
  title = {Buffering {{Negative News}}: {{Individual-level Effects}} of {{Company Visibility}}, {{Tone}}, and {{Pre-existing Attitudes}} on {{Corporate Reputation}}},
  shorttitle = {Buffering {{Negative News}}},
  author = {Jonkman, Jeroen G. F. and Boukes, Mark and Vliegenthart, Rens and Verhoeven, Piet},
  date = {2020-03-03},
  journaltitle = {Mass Communication and Society},
  volume = {23},
  number = {2},
  pages = {272--296},
  publisher = {{Routledge}},
  issn = {1520-5436},
  doi = {10.1080/15205436.2019.1694155},
  url = {https://doi.org/10.1080/15205436.2019.1694155},
  urldate = {2022-02-24},
  abstract = {Building on the agenda-setting theory, this study investigates the effect of corporations’ visibility and tone in news coverage on reputation. More specifically, we examine the buffering role that prior reputation may have for the potential damaging impact of news coverage. Providing a stringent test of causality, data from an automated content analysis of Dutch online and print newspaper coverage (N = 5,235 articles) were linked to individual responses from a three-wave panel survey (N = 3,270 respondents) with repeated measurements of corporate reputation (12 organizations). The analyses show that mere exposure to corporations negatively affects reputation, whereas tone has a positive effect on reputation. It is furthermore shown that the effect of negative news is three times larger than the effect of positive news. Finally, in accordance with research on buffering effects of corporate reputation, we demonstrate that negative news is less influential for people holding more positive existing reputational attitudes.},
  annotation = {\_eprint: https://doi.org/10.1080/15205436.2019.1694155},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Jonkman et al./Jonkman et al. (2020). Buffering Negative News.2.pdf}
}

