  annotation = {\_eprint: https://bpspsychub.onlinelibrary.wiley.com/doi/pdf/10.1348/014466609X449395},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Nigbur et al. (2010). Attitudes, norms, identity and environmental behaviour.pdf}
}

