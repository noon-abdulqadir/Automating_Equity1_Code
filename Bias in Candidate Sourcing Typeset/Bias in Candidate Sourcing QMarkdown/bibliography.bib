  title = {Getting Closer: {{The}} Effects of Personalized and Interactive Online Political Communication},
  shorttitle = {Getting Closer},
  author = {Kruikemeier, Sanne and family=Noort, given=Guda, prefix=van, useprefix=true and Vliegenthart, Rens and family=Vreese, given=Claes H, prefix=de, useprefix=true},
