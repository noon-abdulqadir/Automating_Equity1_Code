@inproceedings{li2021AlgorithmicHiringPractice,
  ids = {li2021AlgorithmicHiringPracticea},
  title = {Algorithmic {{Hiring}} in {{Practice}}: {{Recruiter}} and {{HR Professional}}'s {{Perspectives}} on {{AI Use}} in {{Hiring}}},
  shorttitle = {Algorithmic {{Hiring}} in {{Practice}}},
  booktitle = {Proceedings of the 2021 {{AAAI}}/{{ACM Conference}} on {{AI}}, {{Ethics}}, and {{Society}}},
