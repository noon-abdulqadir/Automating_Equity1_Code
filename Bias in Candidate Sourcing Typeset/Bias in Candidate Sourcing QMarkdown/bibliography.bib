  shorttitle = {Sex Isn’t {{Gender}}},
  author = {Bittner, Amanda and Goodyear-Grant, Elizabeth},
  date = {2017-12},
  journaltitle = {Political Behavior},
  shortjournal = {Polit Behav},
  volume = {39},
