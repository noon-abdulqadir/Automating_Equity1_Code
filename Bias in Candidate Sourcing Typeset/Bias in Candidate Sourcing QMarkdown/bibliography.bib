@article{todorov2008UnderstandingEvaluationFaces,
  ids = {todorovUnderstandingEvaluationFaces2008},
  title = {Understanding Evaluation of Faces on Social Dimensions},
  author = {Todorov, Alexander and Said, Chris P. and Engell, Andrew D. and Oosterhof, Nikolaas N.},
