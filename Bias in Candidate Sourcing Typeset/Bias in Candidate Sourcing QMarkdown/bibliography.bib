  shorttitle = {Attitudes, Norms, Identity and Environmental Behaviour},
  author = {Nigbur, Dennis and Lyons, Evanthia and Uzzell, David},
  date = {2010},
  journaltitle = {British Journal of Social Psychology},
  volume = {49},
