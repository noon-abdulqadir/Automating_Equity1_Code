@book{carver2012AttentionSelfRegulationControlTheory,
  ids = {Carver1981,carverAttentionSelfRegulationControlTheory1981},
  title = {Attention and {{Self-Regulation}}: {{A Control-Theory Approach}} to {{Human Behavior}}},
  shorttitle = {Attention and {{Self-Regulation}}},
  author = {Carver, C. S. and Scheier, M. F.},
