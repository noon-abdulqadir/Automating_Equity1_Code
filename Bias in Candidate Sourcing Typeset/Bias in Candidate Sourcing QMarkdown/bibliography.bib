  title = {A Further Extension of the {{Extended Parallel Process Model}} ({{E-EPPM}}): Implications of Cognitive Appraisal Theory of Emotion and Dispositional Coping Style},
  shorttitle = {A Further Extension of the {{Extended Parallel Process Model}} ({{E-EPPM}})},
