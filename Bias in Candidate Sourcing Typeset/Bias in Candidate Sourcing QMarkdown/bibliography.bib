  shorttitle = {The “{{Big Two}}” in {{Hiring Discrimination}}},
  author = {Veit, Susanne and Arnu, Hannah and Di Stasio, Valentina and Yemane, Ruta and Coenders, Marcel},
  date = {2021-03-06},
