  title = {The “{{Shoulds}}” and “{{Should Nots}}” of {{Moral Emotions}}: {{A Self-Regulatory Perspective}} on {{Shame}} and {{Guilt}}},
  author = {Sheikh, Sana and Janoff-Bulman, Ronnie},
