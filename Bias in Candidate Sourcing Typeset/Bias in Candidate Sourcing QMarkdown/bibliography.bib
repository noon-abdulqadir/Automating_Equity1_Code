  author = {Olesen, Jonas B. and Køber, Lars and Torp-Pedersen, Christian and Gislason, Gunnar H. and Kristensen, Søren L. and Olsen, Anne-Marie S. and Hansen, Peter R. and Fosbøl, Emil L. and Lamberts, Morten and Hansen, Morten L. and Folke, Frederik},
