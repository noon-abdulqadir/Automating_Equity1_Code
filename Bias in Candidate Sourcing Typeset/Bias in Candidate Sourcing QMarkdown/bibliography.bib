@unpublished{shi2020SalienceMarketawareSkill,
  title = {Salience and {{Market-aware Skill Extraction}} for {{Job Targeting}}},
  author = {Shi, Baoxu and Yang, Jaewon and Guo, Feng and He, Qi},
  date = {2020-05-26},
  eprint = {2005.13094},
  eprinttype = {arxiv},
