  keywords = {Computer Science - Computation and Language},
  note = {Comment: ACL 2022 - Theme track},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Hershcovich et al. (2022). Challenges and Strategies in Cross-Cultural NLP.pdf;/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Data Directory/storage/TFR49CIX/2203.html}
}

@article{Hess2004,
