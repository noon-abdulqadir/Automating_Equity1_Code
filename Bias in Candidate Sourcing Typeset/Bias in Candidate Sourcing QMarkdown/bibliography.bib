@article{Pollock1983,
  ids = {ParticipatoryConsequencesInternal1983,pollockParticipatoryConsequencesInternal1983},
  title = {The {{Participatory Consequences}} of {{Internal}} and {{External Political Efficacy}}: {{A Research Note}}},
  author = {Pollock, Philip H.},
