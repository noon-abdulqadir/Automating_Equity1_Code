@article{tangney2007MoralEmotionsMoral,
  ids = {MoralEmotionsMoral2007,Tangney2007,Tangney2007Moral},
  title = {Moral {{Emotions}} and {{Moral Behavior}}},
  author = {Tangney, June Price and Stuewig, Jeff and Mashek, Debra J.},
  date = {2007},
  journaltitle = {Annual Review of Psychology},
  volume = {58},
  number = {1},
