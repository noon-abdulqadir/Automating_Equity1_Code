@article{Carli2016,
  ids = {StereotypesGenderScience2016},
  title = {Stereotypes {{About Gender}} and {{Science}}: {{Women}} ≠ {{Scientists}}},
  author = {Carli, Linda L. and Alawa, Laila and Lee, Yoon Ah and Zhao, Bei and Kim, Elaine},
  date = {2016},
