@article{paluck2009DeferenceDissentDispute,
  ids = {DeferenceDissentDispute2009,Paluck2009,paluckDeferenceDissentDispute2009a},
  title = {Deference, Dissent, and Dispute Resolution: {{An}} Experimental Intervention Using Mass Media to Change Norms and Behavior in Rwanda},
  author = {Paluck, Elizabeth Levy and Green, Donald P.},
  date = {2009},
  journaltitle = {American Political Science Review},
  volume = {103},
