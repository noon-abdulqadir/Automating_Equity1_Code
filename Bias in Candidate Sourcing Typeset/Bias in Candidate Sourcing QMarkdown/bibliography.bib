  title = {Confrontational and Preventative Policy Responses to Terrorism: {{Anger}} Wants a Fight and Fear Wants "Them" to Go Away},
  author = {Skitka, Linda J. and Bauman, Christopher W. and Aramovich, Nicholas P. and Morgan, G. Scott},
