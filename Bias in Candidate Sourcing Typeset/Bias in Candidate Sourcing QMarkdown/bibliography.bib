  keywords = {adoption,human behavior,information security,Internet of Things (IoT),notion,Study 2,trust},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/AlHogail (2018). Improving IoT Technology Adoption through Improving Consumer Trust.pdf}
