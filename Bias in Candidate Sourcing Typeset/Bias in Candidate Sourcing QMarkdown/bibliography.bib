@article{luoma-aho2010MoreDynamicStakeholder,
  title = {Towards a More Dynamic Stakeholder Model: Acknowledging Multiple Issue Arenas},
  shorttitle = {Towards a More Dynamic Stakeholder Model},
  author = {Luoma‐aho, Vilma and Vos, Marita},
  date = {2010-01-01},
  journaltitle = {Corporate Communications: An International Journal},
  volume = {15},
