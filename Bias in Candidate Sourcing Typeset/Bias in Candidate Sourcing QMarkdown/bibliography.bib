@inproceedings{pronoza2016AspectBasedRestaurantInformation,
  title = {Aspect-{{Based Restaurant Information Extraction}} for the {{Recommendation System}}},
  booktitle = {Human {{Language Technology}}. {{Challenges}} for {{Computer Science}} and {{Linguistics}}},
  author = {Pronoza, Ekaterina and Yagunova, Elena and Volskaya, Svetlana},
