@article{Rees2015,
  ids = {GuiltyConscienceMotivating2015,Rees2015Guilty,reesGuiltyConscienceMotivating2015},
  title = {Guilty Conscience: Motivating pro-Environmental Behavior by Inducing Negative Moral Emotions},
  author = {Rees, Jonas H. and Klug, Sabine and Bamberg, Sebastian},
  date = {2015},
  journaltitle = {Climatic Change},
  volume = {130},
  number = {3},
  pages = {439--452},
  issn = {15731480},
  doi = {10.1007/s10584-014-1278-x},
  abstract = {Conceptual frameworks in the realm of climate-related policy, attitudes and behavior frequently argue that moral emotions play a crucial role in mobilizing pro-environmental action. Yet, little is known about the direct impact of moral emotions on environmental attitudes and behavior. Drawing on emotion research in the context of intergroup relations, the current paper investigates the role of guilty conscience (guilt and shame) as well as other emotions (anger, sadness, pride, and emotional coldness) in motivating pro-environmental behavior intentions and actual behavior as a specific form of reparative action. When confronted with human-caused (vs. seemingly natural) environmental damages, participants (N = 114) reported significantly more guilty conscience. Importantly, participants in the human-caused condition were significantly more likely to spontaneously display actual pro-environmental behavior (sign a petition addressing environmental issues). Highlighting its psychological significance in motivating pro-environmental behavior, a guilty conscience mediated the experimental manipulation’s effect on behavioral intentions as well as on actual behavior. We conclude by discussing the potential of moral emotions in developing timely and sustainable climate policies and interventions. © 2014, Springer Science+Business Media Dordrecht.},
  keywords = {Shame and Guilt},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Rees et al. (2015). Guilty conscience.pdf}
}

