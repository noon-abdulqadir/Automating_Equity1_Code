  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Burgers & Beukeboom (2016). Stereotype Transmission and Maintenance Through Interpersonal Communication.pdf}
}

