  keywords = {book reviews,natural language processing,tagging},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Antoniak et al. (2021). Tags, Borders, and Catalogs.pdf}
}

