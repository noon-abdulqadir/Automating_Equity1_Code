}

@article{rossler2011SelectionImpactPress,
  ids = {SelectionImpactPress2011,rosslerSelectionImpactPress2011a},
  title = {Selection and Impact of Press Photography. {{An}} Empirical Study on the Basis of Photo News Factors},
  author = {Rössler, Patrick and Bomhoff, Jana and Haschke, Josef Ferdinand and Kersten, Jan and Müller, Rüdiger},
  date = {2011-01},
  journaltitle = {Communications},
