}

@incollection{oudemulders2020HowEmployersRespond,
  title = {How {{Do Employers Respond}} to an~{{Aging Workforce}}? {{Evidence}} from {{Surveys Among Employers}}, 2009–2017},
  shorttitle = {How {{Do Employers Respond}} to an~{{Aging Workforce}}?},
  booktitle = {Current and {{Emerging Trends}} in {{Aging}} and {{Work}}},
  author = {Oude Mulders, Jaap and Henkens, Kène and family=Dalen, given=Hendrik P., prefix=van, useprefix=true},
