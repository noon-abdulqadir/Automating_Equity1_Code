@article{Ningrum2020,
  ids = {TextMiningOnline2020,ningrumTextMiningOnline2020},
  title = {Text Mining of Online Job Advertisements to Identify Direct Discrimination during Job Hunting Process: {{A}} Case Study in {{Indonesia}}},
  author = {Ningrum, Panggih Kusuma and Pansombut, Tatdow and Ueranantasun, Attachai},
  date = {2020},
  journaltitle = {PLoS ONE},
  volume = {15},
  number = {6},
  eprint = {32497044},
  eprinttype = {pmid},
  publisher = {{Public Library of Science}},
  issn = {19326203},
  doi = {10.1371/journal.pone.0233746},
  abstract = {Discrimination in the workplace is illegal, yet discriminatory practices remain a persistent global problem. To identify discriminatory practices in the workplace, job advertisement analysis was used by previous studies. However, most of those studies adopted content analysis by manually coding the text from a limited number of samples since working with a large scale of job advertisements consisting of unstructured text data is very challenging. Encountering those limitations, the present study involves text mining techniques to identify multiple types of direct discrimination on a large scale of online job advertisements by designing a method called Direct Discrimination Detection (DDD). The DDD is constructed using a combination of N-grams and regular expressions (regex) with the exact match principle of a Boolean retrieval model. A total of 8,969 online job advertisements in English and Bahasa Indonesia, published from May 2005 to December 2017 were collected from bursakerja-jateng.com as the data. The results reveal that the practices of direct discrimination still exist during the job-hunting process including gender, marital status, physical appearances, and religion. The most recurrent type of discrimination which occurs in job advertisements is based on age (66.27\%), followed by gender (38.76\%), and physical appearances (18.42\%). Additionally, female job seekers are found as the most vulnerable party to experience direct discrimination during recruitment. The results exhibit female job seekers face complex jeopardy in particular job positions comparing to their male counterparts. Not only excluded because of their gender, but female job seekers also had to fulfil more requirements for getting an opportunity to apply for the jobs such as being single, still at a young age, complying specific physical appearances and particular religious preferences. This study illustrates the power and potential of optimizing computational methods on a large scale of unstructured text data to analyze phenomena in the social field.},
  langid = {english},
  keywords = {Study 1},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Ningrum et al. (2020). Text mining of online job advertisements to identify direct discrimination.pdf}
}

@article{nir2012CrossnationalDifferencesPolitical,
  ids = {CrossnationalDifferencesPolitical2012,Khalil2012,nirCrossNationalDifferencesPolitical2012},
  title = {Cross-National Differences in Political Discussion: {{Can}} Political Systems Narrow Deliberation Gaps?},
  author = {Nir, Lilach},
  date = {2012},
  journaltitle = {Journal of Communication},
  volume = {62},
  number = {3},
  pages = {553--570},
  issn = {00219916},
  doi = {10.1111/j.1460-2466.2012.01648.x},
  abstract = {Even though many researchers devoted considerable attention to political discussion and its individual-level antecedents and outcomes, insights are based on single-country studies. Cross-national variations were either never studied or implicitly equated to the U.S. context. This study integrates explanations from communication and comparative politics to test whether political system features (e.g., electoral competitiveness and multiple parties) affect the macrosupply of political information, and thus either amplify or diminish the effects of individual characteristics on discussion. Analyses of cross-national data show system features correlate with greater discussion frequency and moderate the contribution of individual differences to discussion. The potential of systems to narrow gaps in mass public discussion and implications for future research are considered in conclusion.\textbackslash nLes différences transnationales dans la discussion politique : les systèmes politiques peuvent-ils réduire les écarts de délibération? Lilach Nir Bien que plusieurs chercheurs aient consacré une attention considérable à la discussion politique et à ses antécédents et résultats au niveau individuel, leurs idées se basent sur des études uninationales. Soit les variations à travers les nations n'étaient pas étudiées, soit elles étaient implicitement comparées au contexte des États-Unis. Cette étude combine les explications de la communication et de la politique comparée pour tester si les caractéristiques d'un système politique (p. ex. la concurrence électorale ou le multipartisme) influencent l'offre macro d'information politique et donc amplifient ou diminuent les effets des caractéristiques individuelles sur la discussion. Des analyses de données transnationales montrent que les caractéristiques du système sont correlées à une plus grande fréquence de discussion, et qu'elles modèrent la contribution des différences individuelles à la discussion. Le potentiel des systèmes à réduire les écarts dans la discussion publique de masse et les conséquences pour la recherche future sont discutés en conclusion. Mots clés : communication politique, comparaison transnationale, délibération et conversation, capital discursif, discussion politique, réseaux sociaux\textbackslash nNationale Unterschiede in politischen Diskussionen: Können politische Systeme die Deliberationslücke verringern? Auch wenn viele Forscher politische Diskussionen, ihren Voraussetzungen und Wirkungen auf Individualebene viel Aufmerksamkeit zukommen lassen haben, basieren diese Studie auf jeweils einem Land. Unterschiede zwischen Ländern wurden entweder nicht untersucht oder implizit mit dem amerikanischen Kontext gleichgesetzt. Diese Studie integriert Ansätze aus der Kommunikationswissenschaft und der vergleichenden Politikwissenschaft, um zu testen, ob Eigenschaften des politischen Systems (z.B. Wahlkampf, Mehrparteiensystem) das Makro-Angebot an politischen Informationen beeinflussen und damit die Wirkung von Individualeigenschaften auf Diskussionen entweder versstärkt oder vermindert. Analysen von ländervergleichenden Daten zeigen, dass Systemeigenschaften mit höherer Diskussionshäufigkeit korrelieren und den Einfluss von Unterschieden auf Individualebene auf Diskussionen moderieren. Das Potential von Systemen, Lücken in öffentlichen Diskussionen zu schließen sowie Implikationen für zukünftige Forschung werden diskutiert. Schlüsselbegriffe: politische Kommunikation, Ländervergleich, Deliberation und Konversation, diskursives Kapital, politische Gespräche, soziale Netzwerke\textbackslash nDiferencias a través de las Naciones en la Discusión Política: Pueden los Sistemas Políticos Reducir la Brecha de la Deliberación? Lilach Nir The Hebrew University of Jerusalem Resumen Aún cuando muchos investigadores dedicaron atención considerable a la discusión política y sus antecedentes individuales y resultados, los entendimientos están basados en estudios de un solo país. Las variaciones a través de las naciones no fueron estudiadas nunca, o equiparadas implícitamente con el contexto de los EE.UU. Este estudio integra las explicaciones de la comunicación y la política comparativa para poner a prueba si las características del sistema político (por ejemplo, la competencia electoral; partidos múltiples) afectan el suministro de información política a nivel macro, y así amplifican o disminuyen los efectos de las características individuales sobre la discusión. Los análisis de los datos multinacionales muestran que las características del sistema se correlacionan con la mayor frecuencia de la discusión, y moderan la contribución de las diferencias individuales en la discusión. El potencial de los sistemas para reducir la brecha en las discusiones públicas masivas y las implicancias para la investigación futura son consideradas en la conclusión. Palabras Claves: Comunicación política; Comparación multinacional; Deliberación y conversación; Capital discursivo; Charla política; Redes sociales},
  isbn = {9781936504220},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Nir (2012). Cross-national differences in political discussion.pdf}
}

