  issn = {13684302},
  doi = {10.1177/1368430205051064},
  abstract = {Participants recalled instances when they felt vicariously ashamed or guilty for another's wrongdoing and rated their appraisals of the event and resulting motivations. The study tested aspects of social association that uniquely predict vicarious shame and guilt. Results suggest that the experience of vicarious shame and vicarious guilt are distinguishable. Vicarious guilt was predicted by one's perceived interdependence with the wrongdoer (e.g. high interpersonal interaction), an appraisal of control over the event, and a motivation to repair the other person's wrongdoing. Vicarious shame was predicted by the relevance of the event to a shared social identity with the wrongdoer, an appraisal of self-image threat, and a motivation to distance from the event. Implications for intergroup behavior and emotion are discussed. Copyright © 2005 SAGE Publications.},
  issue = {2 SPEC. ISS.},
  keywords = {Guilt,Interdependence,Shame,Shame and Guilt,Social identity,Stereotypes},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Lickel et al. (2005). Vicarious shame and guilt.pdf}
}

