@article{Bogusch2016,
  ids = {AnxietyDepressiveSymptoms2016},
  title = {Anxiety and {{Depressive Symptoms}} as {{Mediators}} of {{Trait Mindfulness}} and {{Sleep Quality}} in {{Emerging Adults}}},
  author = {Bogusch, Leah M. and Fekete, Erin M. and Skinta, Matthew D.},
  date = {2016-08-01},
  journaltitle = {Mindfulness},
  shortjournal = {Mindfulness},
  volume = {7},
