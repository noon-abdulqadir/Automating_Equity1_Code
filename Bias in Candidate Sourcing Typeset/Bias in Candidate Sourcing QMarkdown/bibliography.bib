  shorttitle = {The Genesis of Climate Change Activism},
  author = {Roser-Renouf, Connie and Maibach, Edward W. and Leiserowitz, Anthony and Zhao, Xiaoquan},
  date = {2014-07},
  journaltitle = {Climatic Change},
  shortjournal = {Climatic Change},
  volume = {125},
