@misc{moreau_conducting_2020,
  title = {Conducting a {{Meta-Analysis}} in the {{Age}} of {{Open Science}}: {{Tools}}, {{Tips}}, and {{Practical Recommendations}}},
  shorttitle = {Conducting a {{Meta-Analysis}} in the {{Age}} of {{Open Science}}},
  author = {Moreau, David and Gamble, Beau},
  date = {2020-01-07T22:53:02},
  publisher = {{PsyArXiv}},
  doi = {10.31234/osf.io/t5dwg},
  url = {https://psyarxiv.com/t5dwg/},
  urldate = {2022-08-08},
  abstract = {Psychology researchers are rapidly adopting open science practices, yet clear guidelines on how to apply these practices to meta-analysis remain lacking. In this tutorial, we describe why open science is important in the context of meta-analysis in psychology, and suggest how to adopt the three main components of open science: preregistration, open materials, and open data. We first describe how to make the preregistration as thorough as possible—and how to handle deviations from the plan. We then focus on creating easy-to-read materials (e.g., search syntax, R scripts) to facilitate reproducibility and bolster the impact of a meta-analysis. Finally, we suggest how to organize data (e.g., literature search results, data extracted from studies) that are easy to share, interpret, and update as new studies emerge. For each step of the meta-analysis, we provide example templates, accompanied by brief video tutorials, and show how to integrate these practices into the Open Science Framework (https://osf.io/q8stz/).},
  langid = {american},
  keywords = {data sharing,meta-analysis,Meta-science,open materials,open science,preregistration,reliability,replicability,reproducibility,Social and Behavioral Sciences,transparency},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Moreau & Gamble (2020). Conducting a Meta-Analysis in the Age of Open Science.pdf}
}

