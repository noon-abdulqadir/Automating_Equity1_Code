  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ijsa.12260},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Amaral et al. (2019). Why does impression management positively influence interview ratings.pdf}
}

@article{amazeen2020JournalisticInterventionsStructural,
  ids = {Amazeen2020,JournalisticInterventionsStructural2020},
  title = {Journalistic Interventions: {{The}} Structural Factors Affecting the Global Emergence of Fact-Checking},
  shorttitle = {Journalistic Interventions},
