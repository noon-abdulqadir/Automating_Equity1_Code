@article{swim2015PortrayingPerilsPolar,
  ids = {PortrayingPerilsPolar2015,Swim2015,Swim2015Portraying},
  title = {Portraying the {{Perils}} to {{Polar Bears}}: {{The Role}} of {{Empathic}} and {{Objective Perspective-taking Toward Animals}} in {{Climate Change Communication}}},
  shorttitle = {Portraying the {{Perils}} to {{Polar Bears}}},
  author = {Swim, Janet K. and Bloodhart, Brittany},
