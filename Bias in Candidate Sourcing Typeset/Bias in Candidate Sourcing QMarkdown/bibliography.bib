@article{Velotti2017,
  ids = {FacesShameImplications2017,Velotti2017Face,velottiFacesShameImplications2017},
  title = {Faces of {{Shame}}: {{Implications}} for {{Self-Esteem}}, {{Emotion Regulation}}, {{Aggression}}, and {{Well-Being}}},
  author = {Velotti, Patrizia and Garofalo, Carlo and Bottazzi, Federica and Caretti, Vincenzo},
