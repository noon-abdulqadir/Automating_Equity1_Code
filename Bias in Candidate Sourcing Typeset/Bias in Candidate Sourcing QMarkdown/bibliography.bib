@article{liu2021BobaAuthoringVisualizing,
  title = {Boba: {{Authoring}} and {{Visualizing Multiverse Analyses}}},
  shorttitle = {Boba},
  author = {Liu, Yang and Kale, Alex and Althoff, Tim and Heer, Jeffrey},
  date = {2021-02},
