  author = {Makhortykh, Mykola and family=León, given=Ernesto, prefix=de, useprefix=true and Urman, Aleksandra and Christner, Clara and Sydorova, Maryna and Adam, Silke and Maier, Michaela and Gil-Lopez, Teresa},
