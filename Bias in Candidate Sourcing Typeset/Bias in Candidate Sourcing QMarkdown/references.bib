@inproceedings{chohlas-wood2021BlindJusticeAlgorithmically,
  ids = {chohlas-woodBlindJusticeAlgorithmically},
  title = {Blind {{Justice}}: {{Algorithmically Masking Race}} in {{Charging Decisions}}},
  shorttitle = {Blind {{Justice}}},
  booktitle = {Proceedings of the 2021 {{AAAI}}/{{ACM Conference}} on {{AI}}, {{Ethics}}, and {{Society}}},
  author = {Chohlas-Wood, Alex and Nudell, Joe and Yao, Keniel and Lin, Zhiyuan (Jerry) and Nyarko, Julian and Goel, Sharad},
  date = {2021-07-21},
  series = {{{AIES}} '21},
  pages = {35--45},
