@article{lippert2006SupplyChainStudy,
  ids = {lippertSupplyChainStudy2006a},
  title = {A Supply Chain Study of Technology Trust and Antecedents to Technology Internalization Consequences},
  author = {Lippert, Susan K. and Forman, Howard},
