@article{DeBoer2020,
  ids = {HowCitizensAssess2020},
  title = {How {{Do Citizens Assess Street-Level Bureaucrats}}’ {{Warmth}} and {{Competence}}? {{A Typology}} and {{Test}}},
  author = {family=Boer, given=Noortje, prefix=de, useprefix=true},
  date = {2020},
