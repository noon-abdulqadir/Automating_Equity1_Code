@article{altenburger2017AreThereGender,
  ids = {Altenburger2017,AreThereGender,altenburgerAreThereGender,altenburgerAreThereGender2017a,altenburgerAreThereGendera},
  title = {Are {{There Gender Differences}} in {{Professional Self-Promotion}}? {{An Empirical Case Study}} of {{LinkedIn Profiles Among Recent MBA Graduates}}},
  shorttitle = {Are {{There Gender Differences}} in {{Professional Self-Promotion}}?},
  author = {Altenburger, Kristen and De, Rajlakshmi and Frazier, Kaylyn and Avteniev, Nikolai and Hamilton, Jim},
  date = {2017-05-03},
  journaltitle = {Proceedings of the International AAAI Conference on Web and Social Media},
  shortjournal = {ICWSM},
