  keywords = {Explainability,Interdisciplinarity,Job candidate screening,Machine learning,Methodology,Multimodal analysis,notion,Psychology,Study 2},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Liem et al. (2018). Psychology Meets Machine Learning.pdf}
