  keywords = {Algorithmic management,Artificial intelligence (AI),Explainable AI (XAI),Fair and responsible AI,Future of work,Human Intervention,Human resource management,Interpretability,Stakeholder-centered design,To Read,Transparency},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Park et al. (2022). Designing Fair AI in Human Resource Management.pdf}
