  keywords = {fairness,fairness-aware AI,fairness-aware machine learning,interpretability,notion,responsible AI,Study 2},
  annotation = {\_eprint: https://wires.onlinelibrary.wiley.com/doi/pdf/10.1002/widm.1356},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Ntoutsi et al. (2020). Bias in data-driven artificial intelligence systems—An introductory survey.pdf}
