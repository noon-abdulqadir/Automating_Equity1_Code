@article{visser2018AutomationAutonomyImportance,
  title = {From ‘Automation’ to ‘Autonomy’: The Importance of Trust Repair in Human–Machine Interaction},
  shorttitle = {From ‘Automation’ to ‘Autonomy’},
  author = {family=Visser, given=Ewart J., prefix=de, useprefix=false and Pak, Richard and Shaw, Tyler H.},
  date = {2018-10-03},
  journaltitle = {Ergonomics},
  volume = {61},
