@article{alniacik2012RelationshipsCareerMotivation,
  title = {Relationships {{Between Career Motivation}}, {{Affective Commitment}} and {{Job Satisfaction}}},
  author = {Alnıaçık, Ümit and Alnıaçık, Esra and Akçin, Kültigin and Erat, Serhat},
