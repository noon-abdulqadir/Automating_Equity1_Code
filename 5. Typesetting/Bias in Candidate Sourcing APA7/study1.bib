@article{jannari_gendering_2018,
  title = {Gendering Expert Work and Ideal Candidacy in {{Finnish}} and {{Estonian}} Job Advertisements},
  author = {J{\"a}nn{\"a}ri, Jatta and Poutanen, Seppo and Kovalainen, Anne},
