@article{schloegel_age_2018,
  title = {Age Stereotypes in Agile Software Development \textendash{} an Empirical Study of Performance Expectations},
  author = {Schloegel, Uta and Stegmann, Sebastian and Maedche, Alexander and {van Dick}, Rolf},
  year = {2018},
