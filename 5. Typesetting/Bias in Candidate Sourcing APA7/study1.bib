@article{raphael_portrayals_2006,
  title = {Portrayals of {{Information}} and {{Communication Technology}} on {{World Wide Web Sites}} for {{Girls}}},
  author = {Raphael, Chad and Bachen, Christine and Lynn, Kathleen-M. and {Baldwin-Philippi}, Jessica and McKee, Kristen A.},
