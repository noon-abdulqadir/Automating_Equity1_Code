@article{Garg2017,
  ids = {WordEmbeddingsLens2018,gargWordEmbeddingsLens2018,gargWordEmbeddingsQuantify2018},
  title = {Word {{Embeddings}} as a {{Lens}} to {{Quantify}} 100 {{Years}} of {{Gender}} and {{Ethnic Stereotypes}}},
  author = {Garg, Nikhil and Schiebinger, Londa and Jurafsky, Dan and Zou, James},
  year = {2018},
  journal = {Proceedings of the National Academy of Sciences},
  volume = {115},
  number = {16},
  eprint = {1711.08412},
  pages = {E3635--E3644},
  issn = {0027-8424},
  doi = {10.1073/pnas.1720347115},
  abstract = {Word embeddings are ubiquitous in machine learning and natural language processing. Recent works show that the ge-ometry of the word embedding captures gender stereotypes. In this paper, we develop a framework to investigate the tem-poral dynamics of gender and ethnic stereotypes by analyzing how the embedding geometry changes over time. We apply this approach to analyze embeddings trained over 100 years of text data. Moreover, we integrate U.S. Census data over the 20th century and demonstrate that dynamics of the word embedding track closely with demographic and occupation shifts over time. We systematically quantify changes in the embeddings of various classes of occupations and adjectives and their relationship to gender and ethnic groups. While the overall embedding bias has been decreasing, specific occu-pations (e.g. intellectual professions) are still associated with males and adjectives for physical appearance are closer to fe-males. We show that our approach can robustly capture global shifts \textendash{} e.g. the feminist movement in the 1960s and Asian immigration into the U.S. \textendash{} as well as changes in the biases associated with specific words. Temporal analysis of word embeddings thus opens up a powerful new intersection be-tween machine learning and quantitative social science. All of our data, metric and analysis tools are available on GitHub.},
  archiveprefix = {arxiv},
  arxivid = {1711.08412},
  isbn = {1720347115},
  pmid = {29615513},
  annotation = {260 citations (Crossref) [2023-03-17]},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Garg et al. (2018). Word Embeddings as a Lens to Quantify 100 Years of Gender and Ethnic Stereotypes.pdf}
}

