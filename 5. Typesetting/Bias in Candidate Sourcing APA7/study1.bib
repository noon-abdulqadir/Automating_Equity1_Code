  shorttitle = {Extended Pre-Processing Pipeline for Text Classification},
  author = {Cunha, Washington and Canuto, S{\'e}rgio and Viegas, Felipe and Salles, Thiago and Gomes, Christian and Mangaravite, Vitor and Resende, Elaine and Rosa, Thierson and Gon{\c c}alves, Marcos Andr{\'e} and Rocha, Leonardo},
  year = {2020},
