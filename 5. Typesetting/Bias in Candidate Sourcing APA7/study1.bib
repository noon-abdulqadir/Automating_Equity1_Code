  langid = {english},
  keywords = {Study 1},
  annotation = {21 citations (Crossref) [2023-03-17]},
  file = {/Users/nyxinsane/Documents/Google Drive (noon.abdulqadir)/Ref Managers/Zotero/Zotero Base Directory/Files/Froehlich et al. (2020). Gender at Work Across Nations.pdf}
}

@article{fumagalli_ok_2022,
