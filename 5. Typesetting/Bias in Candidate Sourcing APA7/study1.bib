  shorttitle = {`{{Intelligent}}' Lockdown, Intelligent Effects?},
  author = {Yerkes, Mara A. and Andr{\'e}, St{\'e}fanie C. H. and Besamusca, Janna W. and Kruyen, Peter M. and Remery, Chantal L. H. S. and {van der Zwan}, Roos and Beckers, Debby G. J. and Geurts, Sabine A. E.},
